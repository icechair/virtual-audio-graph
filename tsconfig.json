{
  "compilerOptions": {
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "outDir": "dist",
    "sourceMap": false,
    "lib": ["dom", "ESNext", "deno.ns", "deno.unstable"],
    "target": "esnext"
  },
  "include": ["src/**/*"]
}
